{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "6"
        },
        "modules": false,
        "useBuiltIns": false,
        "exclude": [
          "transform-regenerator"
        ]
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": true,
        "helpers": false
      }
    ]
  ]
}
