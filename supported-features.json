{
  "description": "Supported features in ts-binary-compiler",
  "version": "1.0.0",
  "globals": {
    "console": {
      "description": "Console output",
      "members": ["log"]
    },
    "fetch": {
      "description": "HTTP client using libcurl",
      "type": "function",
      "returns": "Promise<Response>",
      "methods": {
        "GET": true,
        "POST": true,
        "PUT": true,
        "DELETE": true,
        "PATCH": true,
        "HEAD": true,
        "OPTIONS": true
      }
    },
    "setTimeout": {
      "description": "Execute callback after delay",
      "type": "function",
      "returns": "number"
    },
    "clearTimeout": {
      "description": "Cancel a setTimeout",
      "type": "function"
    },
    "setInterval": {
      "description": "Execute callback repeatedly",
      "type": "function",
      "returns": "number"
    },
    "clearInterval": {
      "description": "Cancel a setInterval",
      "type": "function"
    },
    "Promise": {
      "description": "ES6 Promise implementation",
      "type": "class"
    },
    "Blob": {
      "description": "Binary Large Object",
      "type": "class",
      "methods": ["text", "arrayBuffer", "slice"],
      "properties": ["size", "type"]
    },
    "FormData": {
      "description": "Multipart form data for file uploads",
      "type": "class",
      "methods": ["append", "delete", "get", "has", "set"],
      "notes": "Supports both string values and Blob/File objects"
    },
    "Response": {
      "description": "HTTP Response (from fetch)",
      "type": "class",
      "methods": ["text", "json", "arrayBuffer", "blob"],
      "properties": ["status", "statusText", "ok", "headers"]
    },
    "JSON": {
      "description": "JSON parsing and stringification",
      "members": ["parse", "stringify"]
    },
    "Object": {
      "description": "JavaScript Object",
      "type": "class"
    },
    "Array": {
      "description": "JavaScript Array",
      "type": "class"
    },
    "String": {
      "description": "JavaScript String",
      "type": "class"
    },
    "Number": {
      "description": "JavaScript Number",
      "type": "class"
    },
    "Boolean": {
      "description": "JavaScript Boolean",
      "type": "class"
    },
    "Math": {
      "description": "Mathematical functions",
      "type": "object"
    },
    "Date": {
      "description": "Date and time",
      "type": "class"
    },
    "RegExp": {
      "description": "Regular expressions",
      "type": "class"
    },
    "Error": {
      "description": "Error object",
      "type": "class"
    },
    "ArrayBuffer": {
      "description": "Binary data buffer",
      "type": "class"
    },
    "Uint8Array": {
      "description": "Typed array",
      "type": "class"
    },
    "globalThis": {
      "description": "Global object reference",
      "type": "object"
    },
    "process": {
      "description": "Process information and environment (limited subset)",
      "type": "object",
      "properties": ["env", "argv", "cwd", "exit", "platform", "arch", "version", "pid"],
      "notes": "Only basic process properties are available. File system and child process operations are not supported."
    },
    "URL": {
      "description": "URL parsing and manipulation (web-compatible)",
      "type": "class",
      "properties": ["href", "protocol", "hostname", "port", "pathname", "search", "hash", "host", "origin", "username", "password"],
      "notes": "Provides parsing and manipulation of URLs following the WHATWG URL Standard"
    },
    "URLSearchParams": {
      "description": "Query string parsing and construction",
      "type": "class",
      "methods": ["append", "get", "toString"],
      "notes": "Provides utilities for working with URL query strings"
    },
    "Headers": {
      "description": "HTTP Headers management",
      "type": "class",
      "methods": ["append", "get", "has", "set", "delete"],
      "notes": "Create and manipulate HTTP headers. Header names are case-insensitive."
    },
    "TextEncoder": {
      "description": "Encode strings to UTF-8 bytes",
      "type": "class",
      "methods": ["encode"],
      "properties": ["encoding"],
      "notes": "Converts strings to Uint8Array of UTF-8 bytes"
    },
    "TextDecoder": {
      "description": "Decode UTF-8 bytes to strings",
      "type": "class",
      "methods": ["decode"],
      "properties": ["encoding"],
      "notes": "Converts Uint8Array or ArrayBuffer to strings"
    },
    "atob": {
      "description": "Decode base64 string to binary string",
      "type": "function",
      "returns": "string",
      "notes": "Standard base64 decoding function"
    },
    "btoa": {
      "description": "Encode binary string to base64",
      "type": "function",
      "returns": "string",
      "notes": "Standard base64 encoding function"
    }
  },
  "unsupportedModules": {
    "fs": "File system operations are not supported in standalone binaries",
    "path": "Path operations are not supported in standalone binaries",
    "child_process": "Child process operations are not supported",
    "http": "HTTP server is not supported (use fetch for HTTP client)",
    "https": "HTTPS server is not supported (use fetch for HTTPS client)",
    "net": "Network operations are not supported",
    "os": "Operating system operations are not supported",
    "crypto": "Node crypto module is not supported",
    "stream": "Node streams are not supported",
    "buffer": "Node Buffer is not supported (use ArrayBuffer/Uint8Array)",
    "events": "Node EventEmitter is not supported",
    "util": "Node util module is not supported",
    "url": "Node URL module is not supported (use URL web API if needed)",
    "querystring": "Node querystring module is not supported",
    "zlib": "Compression is not supported",
    "tls": "TLS/SSL operations are not supported",
    "dns": "DNS operations are not supported",
    "dgram": "UDP/datagram sockets are not supported",
    "cluster": "Cluster operations are not supported",
    "worker_threads": "Worker threads are not supported",
    "perf_hooks": "Performance hooks are not supported",
    "async_hooks": "Async hooks are not supported",
    "inspector": "Inspector is not supported",
    "readline": "Readline is not supported",
    "repl": "REPL is not supported",
    "vm": "VM module is not supported",
    "v8": "V8 specific APIs are not supported",
    "assert": "Node assert module is not supported"
  },
  "notes": [
    "QuickJS provides ES2020 JavaScript support",
    "Fetch API is powered by libcurl",
    "Timers use system time with microsecond precision",
    "Blob API supports text, arrayBuffer, and slice operations",
    "No DOM APIs are available (this is not a browser environment)",
    "No Node.js modules or file system access",
    "Focus on pure computation and HTTP requests"
  ]
}
